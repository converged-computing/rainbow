{
  "name": "Rainbow Scheduler Environment",
  "dockerFile": "Dockerfile",
  "context": "../",

  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      },
      "extensions": [
        "golang.go",
        "zxh404.vscode-proto3"
      ],
    }
  },
  "postStartCommand": "git config --global --add safe.directory /workspaces/rainbow"
}
